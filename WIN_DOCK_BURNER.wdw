     name : Event_CDInsertion
     procedure_id : 1760470316835795080
     type_code : 14
     code : |1-
      // Summary: Manages the insertion event
      // Syntax:
      //[ <Result> = ] Event_CDInsertion ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	boolean: True
      //
      PROCEDURE Event_CDInsertion()
      
      nActionType is int = _EVE.wParam
      
      // The media was inserted into the drive
      IF nActionType = RemovableStorageInserted THEN
      	
      	// Retrieves the information about the inserted media
      	GetMediaInfo()		
      	
      	// Goes back to the first plane
      	MyWindow..Plane = 1		
      	
      	// End of wait
      	IMG_HOURGLASS..Animation = False		
      	
      ELSE
      	
      	// No specific icon
      	IMG_MEDIA_TYPE = ""
      	IMG_MEDIA_TYPE..MouseCursor = curArrow
      	
      	// Modifies the background image of the window
      	MyWindow..BackgroundImage = "\images\burner-empty.png"		
      	
      	// No media found in the drive
      	gbMediaFound = False
      END
      
      RESULT True
