     name : Event_DragAndDropExplorer
     procedure_id : 1760470316835532936
     type_code : 14
     code : |1-
      // Summary: Manages the drag 'n drop of files from the explorer
      // Syntax:
      //Event_DragAndDropExplorer ( [<bFromExplorer> is boolean [, <sListFiles> is string]])
      //
      // Parameters:
      //	bFromExplorer (boolean - default value=1): <specify the role of bFromExplorer>
      //	sListFiles (ANSI string - default value=""): <specify the role of sListFiles>
      // Return value:
      // 	None
      //
      
      PROCEDURE Event_DragAndDropExplorer(LOCAL bFromExplorer is boolean = True, LOCAL sListFiles is string = "")
      
      nNbFilesDropped 		is int
      sFile				is string
      nFileSize			is 8-byte int
      sListErrors			is string
      
      // No media, no burner, exit
      IF gbMediaFound = False _OR_ gbBurnerFound = False THEN
      	RETURN
      END
      
      HourGlass(True)
      
      STC_TITLE = "Adding data..."
      
      IMG_HOURGLASS..Animation = True
      
      // Retrieves the number of files
      IF bFromExplorer THEN
      	nNbFilesDropped = ExplorerRetrieve(_EVE.wParam)
      ELSE
      	nNbFilesDropped = StringCount(sListFiles, CR) + 1
      END
      
      // Retrieves the files
      FOR i = 1 _TO_ nNbFilesDropped
      	
      	// Retrieves the path of the file
      	IF bFromExplorer THEN
      		sFile = ExplorerRetrieve(_EVE.wParam, i)
      	ELSE
      		sFile = ExtractString(sListFiles, i, CR)
      	END
      	
      	// Checks whether it is a file or a directory
      	IF fDirectoryExist(sFile + ["\"]) = True THEN
      		
      		// Retrieves the size of the directory
      		nFileSize = fDirSize(sFile, frRecursive)
      		
      		// Checks whether the addition of the directory will not overflow the size of the media
      		IF CheckCompilationSize(nFileSize) = True THEN
      			
      			STA_INFO = sFile
      			
      			// It's a directory, we must take all the files found in it
      			TH_AddInProgress = True
      			TH_BurnerAddDirectory(sFile, burnerRecursiveAddition + burnerInterruptibleAddition) 
      			
      			// Waits for the end of the addition
      			WHILE TH_AddInProgress
      				ThreadPause(50)
      			END
      			
      			IF TH_Added THEN
      				
      				// Fills the list of files
      				gsListFiles += [CR] + "DIR\" + ExtractString(sFile, lastRank, "\") + TAB + sFile
      				
      				// Fills the progress bar used for filling
      				Event_FillingProgressBar(nFileSize)
      				
      				// Fills the size of the compilation			
      				gnCompilationSize = BurnerCompilationInfo(compilationBlocksUsed)	
      				
      				// Adds the other files/directories
      				CONTINUE
      			END
      			
      		END
      		
      		// If the check failed or if the directory addition failed
      		// Fills the history of errors	
      		sListErrors += [CR] + StringBuild("Unable to add the directory <[%sFile%]>: The size of this directory (%1) is too important therefore it cannot be added (available space: %2)", fSizeFormat(nFileSize), fSizeFormat(gnTotalSize - (gnSizeUsed + gnCompilationSize)))
      		
      	ELSE
      		// Retrieves the size of the file
      		nFileSize = fSize(sFile)
      		
      		// Checks whether the addition of the file will not overflow the size of the media
      		IF CheckCompilationSize(nFileSize) = True THEN
      			
      			STA_INFO = sFile
      			
      			// It's a file, add it to the list
      			TH_AddInProgress = True
      			TH_BurnerAddFile(sFile)
      			
      			// Waits for the end of the addition
      			WHILE TH_AddInProgress
      				ThreadPause(50)
      			END
      			
      			// If the file was successfully added
      			IF TH_Added THEN
      				
      				// Fills the list of files
      				gsListFiles += [CR] + "FILE_LIST\" + fExtractPath(sFile, fFileName+fExtension)
      				
      				// Fills the progress bar used for filling
      				Event_FillingProgressBar(nFileSize)
      				
      				// Fills the size of the compilation			
      				gnCompilationSize += nFileSize
      				
      				// Adds the other files/directories
      				CONTINUE
      			END
      			
      		END
      		
      		// If the check failed or if the directory addition failed
      		// Fills the history of errors	
      		sListErrors += [CR] + StringBuild("Unable to add the file <[%sFile%]>: The size of this file (%1) is too important therefore it cannot be added (available space: %2)", fSizeFormat(nFileSize), fSizeFormat(gnTotalSize - (gnSizeUsed + gnCompilationSize)))
      		
      	END
      END
      
      IMG_HOURGLASS..Animation = False
      IMG_HOURGLASS = IMG_HOURGLASS	
      HourGlass(False)
      
      STC_TITLE = "Ready"
      STA_INFO  = ""
      
      // Errors occurred
      IF sListErrors <> "" THEN
      	
      	// Displays the errors
      	Error(sListErrors)
      END	
