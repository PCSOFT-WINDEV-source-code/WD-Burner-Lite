     code : |1+
      // Summary: Displays the type of media inserted into the burner
      // Syntax:
      //GetMediaType ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //// Automatic process of errors:   run the block of code CASE ERROR
      //
      
      PROCEDURE GetMediaType()
      
      // The burner must be inactive
      IF GetBurnerStatus() <> burnerInactive THEN RETURN
      
      // Stores the type of media
      gnMediaType = BurnerMediaInfo(mediaFormat)
      
      // According to the type of inserted media
      SWITCH gnMediaType
      
      	// Blu-Ray disk
      	CASE mediaFormat_BD_R, mediaFormat_BD_RE, mediaFormat_BD_ROM
      		
      		// 'Blu-ray' icon
      		IMG_MEDIA_TYPE = "\images\type-bluray.png"
      		IMG_MEDIA_TYPE..MouseCursor = curSystemHand
      		IMG_MEDIA_TYPE..ToolTip = "Blu-Ray disk"
      		
      		// Modifies the background image of the window
      		MyWindow..BackgroundImage = "\images\burner-ready.png"
      		gbMediaFound = True
      		BTN_BURN..State = Active
      		
      		BurnerVolumeName("BR-" + Today)
      		
      	// DVD disk
      	CASE mediaFormat_DVD_R_HD, mediaFormat_DVD_RAM, mediaFormat_DVD_RAM_HD, mediaFormat_DVD_ROM, mediaFormat_DVD_ROM_HD, mediaFormat_DVDminusR, ...
      		mediaFormat_DVDminusR_DL, mediaFormat_DVDminusRW, mediaFormat_DVDplusR, mediaFormat_DVDplusR_DL, mediaFormat_DVDplusRW, mediaFormat_DVDplusRW_DL
      		
      		// 'DVD' icon
      		IMG_MEDIA_TYPE = "\images\type-dvd.png"
      		IMG_MEDIA_TYPE..MouseCursor = curSystemHand
      		IMG_MEDIA_TYPE..ToolTip = "DVD disk"
      		
      		// Modifies the background image of the window
      		MyWindow..BackgroundImage = "\images\burner-ready.png"		
      		gbMediaFound = True	
      		BTN_BURN..State = Active
      		
      		BurnerVolumeName("DVD-" + Today)
      		
      	// CD disk
      	CASE mediaFormat_R, mediaFormat_RW
      		
      		// Checks whether it is an Audio CD
      		IF BurnerMediaInfo(mediaType) = mediaType_CDDA_CDROM THEN
      			
      			// 'CD-a' icon
      			IMG_MEDIA_TYPE = "\images\type-cda.png"				
      		ELSE
      			
      			// 'CD' icon
      			IMG_MEDIA_TYPE = "\images\type-cd.png"	
      		END
      		
      		// Modifies the cursor
      		IMG_MEDIA_TYPE..MouseCursor = curSystemHand
      		IMG_MEDIA_TYPE..ToolTip = "Compact-Disk"
      		
      		// Modifies the background image of the window
      		MyWindow..BackgroundImage = "\images\burner-ready.png"
      		gbMediaFound = True
      		BTN_BURN..State = Active
      		
      		BurnerVolumeName("CD-" + Today)
      		
      	// Type of disk unknown or unusable
      	OTHER CASE
      		
      		// No specific icon
      		IMG_MEDIA_TYPE = ""
      		IMG_MEDIA_TYPE..MouseCursor = curArrow
      		IMG_MEDIA_TYPE..ToolTip = ""
      		
      		// Modifies the background image of the window
      		MyWindow..BackgroundImage = "\images\burner-empty.png"		
      		gbMediaFound = False
      		BTN_BURN..State = Grayed
      		
      		// Opens the burner door
      		BurnerEject(True)
      		
      		MyWindow..Plane = 3
      END
