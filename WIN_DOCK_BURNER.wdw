     name : GetBurnerStatus
     procedure_id : 1760470316835729544
     type_code : 14
     code : |1-
      // Summary: Tries to retrieve the status of the burner
      // Syntax:
      //[ <Result> = ] GetBurnerStatus ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	integer: Status of the burner
      //
      PROCEDURE GetBurnerStatus()
      
      nBurnerStatus is int
      nNbAttempts	 is int
      
      // Selects the current burner
      BurnerSelect(COMBO_BURNER)
      
      // Retrieves the status of the burner
      nBurnerStatus = BurnerStatus()
      
      HourGlass(True)
      IMG_HOURGLASS..Animation = True
      
      // If a temporary error occurs
      WHILE nBurnerStatus = -1 _AND_ nNbAttempts <= 3
      	
      	// Increases the number of attempts		
      	nNbAttempts ++
      	
      	// Lets the system perform its own operations
      	Multitask(-100)
      	
      	// Retrieves the status of the burner
      	nBurnerStatus = BurnerStatus()
      END
      
      IMG_HOURGLASS..Animation = False
      IMG_HOURGLASS = IMG_HOURGLASS
      
      HourGlass(False)
      
      RESULT nBurnerStatus
