     name : GetMediaInfo
     procedure_id : 1760470316835467400
     type_code : 14
     code : |1+
      // Summary: Retrieves information about the inserted media
      // Syntax:
      //GetMediaInfo ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE GetMediaInfo()
      
      nBurnerStatus is int
      
      // Retrieves the status of the burner
      nBurnerStatus = GetBurnerStatus()
      
      // The burner must not be currently used
      IF nBurnerStatus <> burnerInactive THEN
      	
      	// The ejection is not allowed
      	BTN_EJECT_MEDIA..State = Grayed
      	BTN_LIST_FILES..State = Grayed
      	BTN_MEDIA_NAME..State = Grayed
      	BTN_ADD_FILE..State = Grayed
      	
      	// No filling
      	Event_FillingProgressBar(0)
      	
      	// No specific icon
      	IMG_MEDIA_TYPE = ""
      	IMG_MEDIA_TYPE..MouseCursor = curArrow
      	
      	gbMediaFound = False	
      ELSE
      	
      	////
      	HourGlass(True)
      	IMG_HOURGLASS..Animation = True
      	
      	// Displays the type (CD, DVD or Blu-Ray)
      	GetMediaType()
      	
      	nSizeMax is 8-byte int
      	nSizeUsed	is 8-byte int
      	
      	// Retrieves the maximum size (in blocks) of the inserted media
      	nSizeMax = BurnerMediaInfo(mediaTotalSize)
      	
      	// Retrieves the size used (in blocks) of the inserted media
      	nSizeUsed = BurnerMediaInfo(mediaSizeUsed)
      	nSizeUsed = Max(0, nSizeUsed)
      	
      	// Case of CD-a
      	IF nSizeMax <= 0 THEN
      		
      		// Define a default size for the CD-a
      		// 2 048 bytes per block
      		//	  75 blocks per second
      		//    60 seconds per minute
      		//    74 minutes per CD
      		// -------------------------
      		// 681 984 000 bytes (i.e. ~650 MB)
      		nSizeMax = 75 * 60 * 74
      	END
      	
      	// Retrieves the total size in bytes
      	nSizeMax = 2048 * nSizeMax
      	
      	// Retrieves the size used in bytes
      	nSizeUsed = 2048 * nSizeUsed
      	
      	// Retrieves the size of headers
      	gnSizeLeads   = Val(BurnerCompilationInfo(compilationBlocksUsed)) * Val(BurnerCompilationInfo(compilationBlockSize))
      	
      	// Keeps the sizes
      	gnTotalSize 		= nSizeMax
      	gnSizeUsed	= nSizeUsed
      	
      	// Actual filling		
      	Event_FillingProgressBar(nSizeUsed, nSizeMax)
      	
      	// Displays the max size
      	STC_MAXSIZE = fSizeFormat(gnTotalSize)
      	
      	// The ejection is allowed
      	BTN_EJECT_MEDIA..State = Active
      	BTN_LIST_FILES..State = Active
      	BTN_MEDIA_NAME..State = Active
      	BTN_ADD_FILE..State = Active
      	
      	IMG_HOURGLASS..Animation = False
      	HourGlass(False)
      	////
      END
     type : 458752
