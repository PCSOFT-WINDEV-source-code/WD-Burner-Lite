  -
    name : BTN_LIST_FILES
    identifier : 0x286e726d044ef088
    internal_properties : 
    properties :
     x : 59
     y : 178
     width : 24
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAABZcJgJpQtomZNYyZP80H6l9hACuEOIWhFbzCLIhLM33/yhFS0KuZ8bNCu8QbPmNa7Dx83Lpm+MH4KHi4jJvfvO8FzKoF+0cgDICedaCs6hOFZ7HmYotBId8PlI8FQahZ9oFHOHycxAu9GzjjWQ1dv3bi2nedNej+fqj/XI7dvWVQ==
        code : |1-
         
         // Empty list
         IF gsListFiles = "" THEN
         	
         	Info("No files to burn")
         	RETURN	
         END
         
         HourGlass(True)
         TreeDeleteAll(TREE_LISTFILES)
         
         sFile is string
         sType	 is string
         sVolume	 is string = BurnerVolumeName()
         sList	 is string
         sElement is string
         sDir	 is string
         
         TreeAdd(TREE_LISTFILES, sVolume, "\images\cd.gif", "\images\cd.gif")
         
         // For each file of the list
         FOR EACH string sFile of gsListFiles SEPARATED BY CR
         	
         	// Extracts the type (file or directory)
         	sType = ExtractString(sFile, 1, "\")
         	
         	SWITCH sType
         	
         		// Directory
         		CASE "ROW"
         			
         			// Extracts the name of the directory
         			sDir 	 = ExtractString(ExtractString(sFile, 1, TAB), 2, "\")
         			sFile = ExtractString(sFile, 2, TAB)
         			
         			// Retrieves the list of files/directories
         			sList = fListFile(sFile + ["\"] + "*.*", frRecursive)
         			
         			FOR EACH string sElement of sList SEPARATED BY CR
         				
         				// Extracts the name of the element
         				sElement = ExtractString(sElement, 2, sFile + ["\"])
         				
         				// Adds the file to the treeview
         				TreeAdd(TREE_LISTFILES, sVolume + TAB + sDir + TAB + Replace(sElement, "\", TAB), "\images\file.gif", "\images\file.gif")
         				
         			END
         			
         		// File
         		CASE "FIL"
         			
         			// Extracts the name of the file
         			sFile = ExtractString(sFile, 2, "\")
         			
         			// Adds the file to the treeview
         			TreeAdd(TREE_LISTFILES, sVolume + TAB + sFile, "\images\file.gif", "\images\file.gif")
         			
         	END
         	
         END
         
         // Expands the root
         TreeExpand(TREE_LISTFILES, sVolume)
         
         HourGlass(False)
         
         AnimationPrepare(MyWindow)
         
         // Displays the plane containing the list of files
         MyWindow..Plane = 5
         MyWindow..Height = 400
         
         // Moves the window up if it cannot be entirely displayed
         IF MyWindow..Y + MyWindow..Height > SysYRes - 40 THEN
         	MyWindow..Y = SysYRes() - 40 - MyWindow..Height
         END
         
         AnimationPlay(1)
