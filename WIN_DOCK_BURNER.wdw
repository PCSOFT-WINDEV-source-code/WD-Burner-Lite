     name : Event_FillingProgressBar
     procedure_id : 1760470316835926152
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //Event_FillingProgressBar (<nAdd> [, <nTotal>])
      //
      // Parameters:
      //	nAdd: <specify the role of nAdd>
      //	nTotal: <specify the role of nTotal>
      // Return value:
      // 	None
      //
      PROCEDURE Event_FillingProgressBar(LOCAL nAdd, LOCAL nTotal = PROGBAR_FILL..MaxValue)
      
      nPercentageUsed is int
      nSizeOccupied		is 8-byte int
      
      // Modifies the upper bound
      IF nTotal <> PROGBAR_FILL..MaxValue THEN
      	PROGBAR_FILL..MaxValue = nTotal
      END
      
      // Fills the progress bar
      IF nAdd = 0 THEN
      	PROGBAR_FILL = 0
      ELSE
      	PROGBAR_FILL += nAdd		
      END	
      
      // Retrieves the space occupied (real)
      nSizeOccupied = Val(BurnerCompilationInfo(compilationBlocksUsed)) * Val(BurnerCompilationInfo(compilationBlockSize))
      
      // Displays the size used (without the headers)
      STC_SIZEUSED = fSizeFormat(PROGBAR_FILL)
      
      // Calculates the percentage of disk use
      nPercentageUsed = (nSizeOccupied * 100 / nTotal)
      
      nColor  is int = RGB(134, 227, 134)
      
      // Decrements the color according to the percentage
      nColor = HSL(ColorHue(nColor) * Max(0, (100 - (nPercentageUsed + modulo(nPercentageUsed, 10)))) / 100, 50, ColorSaturation(nColor) + modulo(nColor, 10))
      
      // Modifies the color of the caption (green: space ok, and becomes more and more red as the space is becoming insufficient)
      STC_SIZEUSED..Color = nColor
      
      WinRedraw()
