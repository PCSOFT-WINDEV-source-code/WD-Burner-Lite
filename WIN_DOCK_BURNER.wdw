     name : Event_BurnInProgress
     procedure_id : 1760470316836122760
     type_code : 14
     code : |1-
      // Summary: Manages the burn event
      // Syntax:
      //Event_BurnInProgress (<nEvent> is int [, <sEventMessage> [, ...]])
      //
      // Parameters:
      //	nEvent (integer): Event
      //	sEventMessage (default value=""): Message that must be associated with the event
      // 	<*>: Other data of the event
      // Return value:
      // 	None
      //
      PROCEDURE Event_BurnInProgress(LOCAL nEvent is int, LOCAL sEventMessage = "", *)
      
      STC_TITLE = "Burn in progress..."
      
      // End of burn
      IF nEvent IN (burnerNotificationCancel, burnerNotificationError, burnerNotificationEndBurning) THEN
      	
      	// Re-enables the controls
      	IMG_ANIMDVD..Visible 	= False
      	IMG_ANIMDVD..Animation 	= False
      	IMG_ANIMDVD				= IMG_ANIMDVD
      	BTN_BURN..State 		= Active
      	BTN_EJECT_MEDIA..State = Active
      	BTN_LIST_FILES..State= Active
      	BTN_MEDIA_NAME..State		= Active
      	BTN_ADD_FILE..State	= Active
      	IMG_MEDIA_TYPE..State 	= Active
      	STC_TITLE				= "Ready"
      	STA_INFO 				= ""
      	PROGBAR_BURN..Visible	= False
      	
      	SWITCH nEvent 
      	
      		// Abandon
      		CASE burnerNotificationCancel
      			Warning("The burn process was canceled")
      			
      		// Error	
      		CASE burnerNotificationError
      			Error("An error occurred during the burn operation.", sEventMessage)
      			
      		// End of burn
      		CASE burnerNotificationEndBurning
      			Info("The burn process is over")
      			
      	END
      	
      	// Restarts the detection
      	PostMessage(Handle(), "END_BURN", nEvent, 0)
      	
      ELSE
      	
      	// According to the event
      	SWITCH nEvent
      	
      		// The burner prepares the burn process
      		CASE burnerNotificationPrepareBurning
      			STA_INFO = "Preparing..."
      			
      			// Displays the progress bar
      			PROGBAR_BURN..Visible	= True
      			
      		// The burner ends the burn process
      		CASE burnerNotificationCloseCD
      			STA_INFO = "End of burn..."
      			
      			// Hides the progress bar
      			PROGBAR_BURN..Visible	= False
      			
      		// The burner is currently writing
      		CASE burnerNotificationBurnTrack, burnerNotificationBurnBlock
      			STA_INFO = "Write operation in progress..."
      			
      			// Fills the progress bar
      			PROGBAR_BURN..Value 	= MyParameters[4]	// Current block/track
      			PROGBAR_BURN..MaxValue = MyParameters[5]	// Total blocks/tracks
      			WinRedraw()
      			
      		// Unknown event
      		OTHER CASE
      			STA_INFO = ""
      			
      	END	
      	
      END
