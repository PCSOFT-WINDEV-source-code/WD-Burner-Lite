     name : Event_DetectCDInsertion
     procedure_id : 1760470316835664008
     type_code : 14
     code : |1-
      // Summary: Manages the insertion of a CD
      // Syntax:
      //Event_DetectCDInsertion (<sDriveLetter> is string, <nActionType> is int)
      //
      // Parameters:
      //	sDriveLetter (string): <specify the role of sDriveLetter>
      //	nActionType (integer): <specify the role of nActionType>
      // Return value:
      // 	None
      //
      PROCEDURE Event_DetectCDInsertion(LOCAL sDriveLetter is string, nActionType is int)
      
      
      
      // Processes the insertion event only if it is a CD-ROM drive or a burner
      IF fDriveInfo(sDriveLetter, fdDriveType) <> fdCDRom THEN RETURN
      
      // If the drive is not the one of the burner, do not process the event
      IF NOT gsBurnerPath [= sDriveLetter THEN RETURN
      
      // Manages the insertion event
      // (Use PostMessage to run the CDInsertion procedure; indeed, the insertion event being received in a process parallel to the application, 
      // the status of the burner cannot be retrieved. Therefore, PostMessage must be used to inform the main process of the application)
      PostMessage(Handle(), "INSERTION_CDROM", nActionType, 0)
