        code : |1+
         
         IF gbMediaFound = False THEN
         	
         	// Opens the burner door
         	BurnerEject(True)
         	
         	// Waits for a media
         	MyWindow..Plane = 3
         	RETURN
         	
         END
         
         // No file to burn
         IF gsListFiles = "" THEN
         	
         	Warning("No file to burn")
         	RETURN
         END
         
         BTN_BURN..State = Grayed
         Multitask()
         
         // End of detection
         fDetectRemovableStorage("")
         
         IMG_ANIMDVD..Visible 	= True
         IMG_ANIMDVD..Animation 	= True
         BTN_EJECT_MEDIA..State = Grayed
         BTN_MEDIA_NAME..State 	= Grayed
         BTN_LIST_FILES..State= Grayed
         BTN_ADD_FILE..State = Grayed
         IMG_MEDIA_TYPE..State 	= Inactive
         
         nBurnOptions is int
         IF InTestMode() THEN
         	
         	// In test mode: simulate the burn process
         	nBurnOptions = burnerSimulate
         END
         
         // Eject automatically at the end of burn
         nBurnOptions += burnerAutoEject
         
         // Starts the burn (eject the media at the end of burn
         IF BurnerSave(nBurnOptions, Event_BurnInProgress) THEN
         	
         	// Clears the list of files
         	gsListFiles			= ""	
         END
