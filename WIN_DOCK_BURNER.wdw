     name : GetBurnerInfo
     procedure_id : 1760470316835598472
     type_code : 14
     code : |1-
      // Summary: Retrieves information about the burner
      // Syntax:
      //GetBurnerInfo ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE GetBurnerInfo()
      
      sListBurners 	is string
      sBurner	  	is string
      sIdentifier	is string
      sBurnerName		is string
      
      STC_TITLE = "Initialization..."
      
      // Retrieves the list of burners installed
      sListBurners = BurnerList(True)
      
      // Browses the list of burners
      FOR EACH string sBurner of sListBurners SEPARATED BY CR
      	
      	// Extracts the identifier of the burner
      	sIdentifier = ExtractString(sBurner, 1)
      	
      	// Extracts the name of the burner
      	sBurnerName = ExtractString(sBurner, 2) + [" "] + ExtractString(sBurner, 3)
      	
      	ListAdd(COMBO_BURNER, gLink(sIdentifier) + sBurnerName)		
      END
      
      STC_TITLE = "Selecting the burner..."
      
      // If a single burner is found, select it automatically
      IF COMBO_BURNER..Occurrence = 1 THEN
      	
      	// Selects the first burner
      	ListSelectPlus(COMBO_BURNER, 1)
      	BurnerSelect(COMBO_BURNER)
      	
      	// Automates the burner selection
      	ExecuteProcess(BTN_VALIDATE_BURNER, trtClick)
      	
      	// If no burner is found
      ELSE IF COMBO_BURNER..Occurrence = 0 THEN
      	
      	// The ejection is not allowed
      	BTN_EJECT_MEDIA..State = Grayed
      	BTN_LIST_FILES..State = Grayed
      	BTN_MEDIA_NAME..State = Grayed
      	BTN_ADD_FILE..State = Grayed
      	
      	// No filling
      	Event_FillingProgressBar(0)
      	
      	// No specific icon
      	IMG_MEDIA_TYPE = ""
      	IMG_MEDIA_TYPE..MouseCursor = curArrow
      	
      	// No burner
      	gbBurnerFound = False
      	
      	// Displays an error
      	Error("No burner is installed on this computer")				
      	RETURN
      	
      	// Display the list of burners
      ELSE
      	
      	// Displays the first plane of the window used to select the burner to use
      	MyWindow..Plane = 2
      	ComboOpen(COMBO_BURNER)
      END
      
      // Enables the detection of media insertion into the drives
      fDetectRemovableStorage(Event_DetectCDInsertion)
      
      END :
      STC_TITLE = "Ready"
